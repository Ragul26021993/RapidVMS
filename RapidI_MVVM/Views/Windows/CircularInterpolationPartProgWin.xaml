<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="Rapid.Windows.CircularInterpolationPartProgWin"
        Title="CircularInterpolationPartProgWin" Height="150" Width="250">
	<Grid Background="{DynamicResource PanelBackground}">
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
			<ColumnDefinition/>
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition Height="0.25*"/>
			<RowDefinition Height="0.5*"/>
			<RowDefinition Height="0.25*"/>
		</Grid.RowDefinitions>
		<TextBlock Text="Take Point" Grid.Row="1" HorizontalAlignment="Right" Margin="0,0,10,0" VerticalAlignment="Center"/>
		<Button x:Name="TakePointBtn" Content="1" Style="{DynamicResource SimpleNormalButton}" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="Black" Grid.Row="1" Grid.Column="1" Width="60" Height="40" HorizontalAlignment="Left" Margin="10,0,0,0"/>
	</Grid>
</Window>
