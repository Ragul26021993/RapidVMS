	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:Options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
	mc:Ignorable="d"
	x:Class="Rapid.Utilities.LastMeasurementDG"
	x:Name="UserControl"
	d:DesignWidth="400" d:DesignHeight="60">
	<DataGrid x:Name="dgvProgramSteps" IsReadOnly="True" CanUserResizeColumns="False" RowHeaderWidth="0" RowHeight="58" AlternatingRowBackground="#FFC4E5FF" CanUserResizeRows="False" HorizontalGridLinesBrush="{x:Null}" VerticalGridLinesBrush="{x:Null}" Background="#FFD2E1FF" RowBackground="#FF9CB2E0" Foreground="Black" HeadersVisibility="None" GridLinesVisibility="None" SelectionMode="Single" CanUserReorderColumns="False" FontWeight="Bold" VerticalContentAlignment="Center" Grid.Column="0" Grid.Row="0" FontSize="36"/>
	<Grid x:Name="GradeSelectionGrid" Background="#FFD2E1FF" Visibility="Hidden">
