		<Button x:Name="OkBtn" Content="OK" HorizontalAlignment="Right" Height="30" Margin="0,0,100,0" Width="70"/>
		<Button x:Name="CancelBtn" Content="Cancel" HorizontalAlignment="Right" Height="30" Margin="0,0,10,0" Width="70"/>
	</Grid>
</Grid>